          <property>metrics/cbarw-ft</property>
          <table>
            <independentVar lookup="row">aero/alpha-wing-rad</independentVar>
            <tableData>
              -0.400   0.55
              -0.305   0.58
              -0.285   0.58
               0.285  -0.58
               0.305  -0.58
               0.400  -0.55
            </tableData>
          </table>
          <value>0.7</value>
          <!-- 
              stall and spin (5):
