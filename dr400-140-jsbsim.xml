          -->
          <table> 
            <independentVar lookup="row">aero/alpha-deg</independentVar>
            <tableData>
               20    1  
               25    0.8
               35    0.7
            </tableData>
            
          </table>
        </product>
      </function>

      <function name="aero/coefficient/Cmde">
